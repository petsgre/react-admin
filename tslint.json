{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "src/utils/**/*.js",
      "mock/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "jsx-boolean-value": false,
    "member-access": false,
    "object-literal-sort-keys": false
  },
  "jsRules": {
    "no-console": false
  }
}